#include <curses.h>
#include <iostream>

using namespace std;

//void title_slide();
//void

int main()
{
	int ch;

	initscr();			/* Start curses mode 		*/
	raw();				/* Line buffering disabled	*/
	keypad(stdscr, TRUE);		/* We get F1, F2 etc..		*/
	noecho();			/* Don't echo() while we do getch */

	

	endwin();			/* End curses mode		  */

	return 0;
}
